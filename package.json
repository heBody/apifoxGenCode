{
  "private": true,
  "name": "apifoxgencode",
  "version": "1.0.0",
  "description": "apifox gen code",
  "main": "dist/apifox-gencode.js",
  "bin": {
    "apifox-gen": "dist/apifox-gencode.js"
  },
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "esno src/index.ts",
    "go": "npm run build & apifox-gen",
    "start": "rollup -c -w",
    "build": "rimraf dist && rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && pnpm run update-readme && git add CHANGELOG.md README.*",
    "release": "bumpp -x \"npm run changelog\" --all --tag --commit --push && npm publish",
    "lint": "eslint --ignore-path .gitignore '**/*.ts'",
    "test": "esno src/Gen.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heBody/apifoxGenCode.git"
  },
  "bugs": {
    "url": "https://github.com/heBody/apifoxGenCode/issues"
  },
  "keywords": [
    "apifox",
    "gen code"
  ],
  "author": "heshuaibiao",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-github-releaser": "^3.1.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "esno": "^0.14.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  },
  "dependencies": {
    "axios": "^0.26.0",
    "commander": "^9.0.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "prompts": "^2.4.2"
  }
}
